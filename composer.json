{
    "name": "Jlndk/LaravelRealtime",
    "type": "library",
    "description": "A realtime library for laravel without compromises, using WAMPv2!",
    "keywords": [
        "laravel",
        "realtime"
    ],
    "homepage": "https://github.com/Jlndk/LaravelRealtime",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Lindenskov Nielsen",
            "email": "jonas@jonasln.dk",
            "homepage": "http://jonasln.dk",
            "role": "Author"
        },
        {
            "name": "Thomas Schauser",
            "email": "thomas.schauser@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Jlndk\\LaravelRealtime\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jlndk\\LaravelRealtime\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
